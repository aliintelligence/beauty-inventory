# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Scraper Configuration
USE_REAL_SCRAPERS=false  # Set to true to use real web scrapers instead of mock data
SCRAPER_RATE_LIMIT_MS=5000  # Delay between scraper requests
SCRAPER_MAX_RETRIES=3  # Maximum retry attempts for failed scrapes

# Proxy Configuration (optional, for production scraping)
PROXY_URL=  # Optional proxy server for scraping
PROXY_USERNAME=
PROXY_PASSWORD=

# Development Settings
NODE_ENV=development